{
  "name": "angular-portfolio",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ng:update": "ng update @angular/core@16 @angular/cli@16 @angular-eslint/schematics@16 @nguniversal/express-engine@16",
    "dev:ssr": "ng run AngularPortfolio:serve-ssr",
    "serve:ssr": "node dist/AngularPortfolio/server/main.js",
    "build:ssr": "ng build && ng run AngularPortfolio:server",
    "prerender": "ng run AngularPortfolio:prerender --routes-file ./dev/routes.txt",
    "vercel-build": "npm run build:ssr",
    "contentful-typescript-codegen": "contentful-typescript-codegen --output src/app/api/generated/contentful/contentful.d.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.6",
    "@angular/common": "^16.1.6",
    "@angular/compiler": "^16.1.6",
    "@angular/core": "^16.1.6",
    "@angular/forms": "^16.1.6",
    "@angular/platform-browser": "^16.1.6",
    "@angular/platform-browser-dynamic": "^16.1.6",
    "@angular/platform-server": "^16.1.6",
    "@angular/router": "^16.1.6",
    "@angular/service-worker": "^16.1.6",
    "@contentful/rich-text-html-renderer": "^16.1.0",
    "@nguniversal/express-engine": "^16.1.1",
    "contentful": "^10.3.6",
    "express": "^4.15.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.5",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "~16.1.5",
    "@angular/compiler-cli": "^16.1.6",
    "@nguniversal/builders": "^16.1.1",
    "@tailwindcss/typography": "^0.5.9",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.17.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "contentful-cli": "^2.6.30",
    "contentful-management": "^10.39.2",
    "contentful-typescript-codegen": "^3.4.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.26",
    "prettier": "^2.4.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.3"
  }
}
